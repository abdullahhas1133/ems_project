<%= form_for [:admin, @coupon] do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :value %><br />
    <%= f.text_field :value, autofocus: true, autocomplete: "value" %>
  </div>
  <%= f.submit %>
<% end %>
